{
  "name": "trading-info-miniapp",
  "version": "1.0.0",
  "description": "Telegram Mini App for educational content with Obsidian-like interface",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"node server.js\" \"cd client && npm start\"",
    "build": "cd client && npm install && npm run build",
    "start": "node server.js",
    "install-client": "cd client && npm install",
    "serve": "npm run build && npm start",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "marked": "^5.1.1",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
      "path": "^0.12.7",
      "winston": "^3.11.0",
      "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  },
  "private": true
}
